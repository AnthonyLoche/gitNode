{
    "version": 2,
    "builds": [
      {
        "src": "app.js", 
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/data",
        "methods": ["POST"],
        "dest": "/app.js"
      },
      {
        "src": "/(.*)",
        "dest": "/app.js" 
  
      }
    ]
  }